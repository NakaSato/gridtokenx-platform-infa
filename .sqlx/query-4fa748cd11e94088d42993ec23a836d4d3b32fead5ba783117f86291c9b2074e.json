  "query": "\n            SELECT id, user_id, notification_type as \"notification_type!: NotificationType\",\n                   title, message, data, read as \"read!\", created_at as \"created_at!\"\n            FROM notifications\n            WHERE user_id = $1 AND read = false\n            ORDER BY created_at DESC\n            LIMIT $2 OFFSET $3\n            ",
